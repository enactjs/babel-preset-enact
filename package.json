{
  "name": "babel-preset-enact",
  "version": "1.0.0",
  "description": "Babel preset used by Enact",
  "repository": {
    "type": "git",
    "url": "https://github.com/enactjs/babel-preset-enact.git"
  },
  "homepage": "https://github.com/enactjs/babel-preset-enact.git",
  "license": "Apache-2.0",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "prettier": {
    "printWidth": 120,
    "useTabs": true,
